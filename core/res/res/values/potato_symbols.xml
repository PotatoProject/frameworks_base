<resources>
  <!-- Keyhandler -->
  <java-symbol type="string" name="config_deviceKeyHandlerLib" />
  <java-symbol type="string" name="config_deviceKeyHandlerClass" />

  <!-- Settings icon tint color -->
  <java-symbol type="color" name="settings_icon_tint_color" />

  <!-- Auto fill -->
  <java-symbol type="color" name="autofill_save_title_color" />
  <java-symbol type="color" name="autofill_autofill_save_custom_subtitle_color" />

  <!-- Notification info -->
  <java-symbol type="color" name="notification_info_button_text" />

  <!-- Whether device supports an alt. ambient display -->
  <java-symbol type="bool" name="config_alt_ambient_display"/>

  <!-- Vendor mismatched fingerprint warning -->
  <java-symbol type="string" name="system_error_vendorprint" />

  <!-- Whether device has dash charging support -->
  <java-symbol type="bool" name="config_hasDashCharger" />

  <!-- Whether device has turbo power charging support -->
  <java-symbol type="bool" name="config_hasTurboPowerCharger" />

  <!-- Whether device has super charging support -->
  <java-symbol type="bool" name="config_hasSuperCharger" />

  <!-- Proximity check on screen on -->
  <java-symbol type="bool" name="config_proximityCheckOnWake" />

  <!-- Proximity check timeout -->
  <java-symbol type="integer" name="config_proximityCheckTimeout" />

  <!-- Proximity check on screen on default -->
  <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />

  <!-- Gesture Sensor -->
  <java-symbol type="bool" name="config_enableGestureService" />

  <!-- App error dialog -->
  <java-symbol type="id" name="aerr_copy" />
  <java-symbol type="string" name="url_copy_success" />
  <java-symbol type="string" name="url_copy_failed" />

  <!-- Toast text and bg colors -->
  <java-symbol type="color" name="toast_bg_color" />
  <java-symbol type="color" name="toast_text_color" />

  <!-- Forcefully tint notification icon text -->
  <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />
  <java-symbol type="bool" name="config_useDarkBgNotificationTinting_override" />
  <java-symbol type="bool" name="config_notificationTinting_override" />

  <!-- HW keys binding -->
  <java-symbol type="anim" name="custom_app_in" />
  <java-symbol type="anim" name="custom_app_out" />
  <java-symbol type="integer" name="config_deviceHardwareKeys" />

  <!-- Button backlight -->
  <java-symbol type="integer" name="config_buttonBrightnessSettingDefault" />
  <java-symbol type="bool" name="config_deviceHasVariableButtonBrightness" />

  <java-symbol type="array" name="config_blockPackagesSensorDrain" />

  <!-- From SignalStrength -->
  <java-symbol type="integer" name="config_LTE_antenna_bar_quantity_metric" />
  <java-symbol type="integer" name="config_LTE_RSRP_threshold_type" />

  <java-symbol type="bool" name="config_multiColorNotificationLed" />

  <!-- Full screen aspect ratio -->
  <java-symbol type="bool" name="config_haveHigherAspectRatioScreen" />
  <java-symbol type="dimen" name="config_screenAspectRatio" />

  <!-- Whether notify fingerprint client of successful cancelled authentication -->
  <java-symbol type="bool" name="config_notifyClientOnFingerprintCancelSuccess" />

  <!-- Package Manager -->
  <java-symbol type="array" name="config_disabledComponents" />
  <java-symbol type="array" name="config_forceEnabledComponents" />

  <!-- Vendor signatures -->
  <java-symbol type="array" name="config_vendorPlatformSignatures" />

  <!-- Power menu -->
  <java-symbol type="string" name="reboot_to_recovery_title" />
  <java-symbol type="string" name="reboot_to_recovery_message" />
  <java-symbol type="string" name="reboot_to_bootloader_title" />
  <java-symbol type="string" name="reboot_to_bootloader_message" />
  <java-symbol type="string" name="reboot_title" />
  <java-symbol type="string" name="reboot_message" />
  <java-symbol type="color" name="power_menu_power_off" />
  <java-symbol type="color" name="power_menu_restart" />
  <java-symbol type="color" name="power_menu_restart_advanced" />
  <java-symbol type="color" name="power_menu_advanced_bootloader" />
  <java-symbol type="color" name="power_menu_advanced_recovery" />
  <java-symbol type="color" name="power_menu_advanced_system_restart_ui" />
  <java-symbol type="color" name="power_menu_lockdown" />
  <java-symbol type="color" name="power_menu_screenshot" />
  <java-symbol type="array" name="config_custom_globalActionsList" />
  <java-symbol type="color" name="power_menu_hot_reboot" />

  <!-- Minimum time in milliseconds between power button presses so it wil e considered
       as a camera launch. To avoid false triggers.
  -->
  <java-symbol type="integer" name="config_cameraPowerDoubleTapMinTime" />

  <!-- Weather icons -->
  <java-symbol type="drawable" name="weather_clear_night" />
  <java-symbol type="drawable" name="weather_cloudy" />
  <java-symbol type="drawable" name="weather_foggy" />
  <java-symbol type="drawable" name="weather_isolated_scattered_thunderstorms_night" />
  <java-symbol type="drawable" name="weather_isolated_scattered_thunderstorms" />
  <java-symbol type="drawable" name="weather_mostly_clear_night" />
  <java-symbol type="drawable" name="weather_mostly_cloudy_night" />
  <java-symbol type="drawable" name="weather_mostly_cloudy" />
  <java-symbol type="drawable" name="weather_mostly_sunny" />
  <java-symbol type="drawable" name="weather_partly_cloudy_night" />
  <java-symbol type="drawable" name="weather_partly_cloudy" />
  <java-symbol type="drawable" name="weather_rain" />
  <java-symbol type="drawable" name="weather_scattered_showers_night" />
  <java-symbol type="drawable" name="weather_scattered_showers" />
  <java-symbol type="drawable" name="weather_snow" />
  <java-symbol type="drawable" name="weather_sunny" />
  <java-symbol type="drawable" name="weather_thunderstorms" />
  <java-symbol type="drawable" name="weather_windy" />

  <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
  <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

  <!-- Whether device has a physical display cutout -->
  <java-symbol type="bool" name="config_physicalDisplayCutout" />

  <!-- Ambient display music ticker -->
  <java-symbol type="bool" name="config_canForceDozeNotifications" />

  <!-- Navigation gestures -->
  <java-symbol type="dimen" name="nav_gesture_swipe_min_length" />
  <java-symbol type="dimen" name="nav_gesture_long_press_max_length" />

  <!-- ADB notification -->
  <java-symbol type="string" name="adb_net_active_notification_title" />
  <java-symbol type="string" name="adb_both_active_notification_title" />
  <java-symbol type="string" name="adb_active_generic_notification_message" />

  <!-- System-wide recents task view roundness to apply to all the recents view.
       this value will apply on Launcher3 quickstep view and also the normal recents -->
  <java-symbol type="dimen" name="recents_task_view_rounded_corners_radius" />

  <!-- Battery LED -->
  <java-symbol type="bool" name="config_intrusiveBatteryLed" />
  <java-symbol type="bool" name="config_multiColorBatteryLed" />
  <java-symbol type="bool" name="config_ledCanPulse" />

  <!-- Flashlight proximity check -->
  <java-symbol type="integer" name="config_flashlightProximityTimeout" />

  <!-- Icon packs -->
  <java-symbol type="drawable" name="adaptive_icon_drawable_wrapper" />

  <java-symbol type="bool" name="config_waitForMpctlOnBoot" />
  
  <!-- Pocket Lock -->
  <java-symbol type="layout" name="pocket_lock_view_layout" />
  <java-symbol type="id" name="pocket_hint_container" />
  <java-symbol type="id" name="pocket_hint_image" />
  <java-symbol type="id" name="pocket_hint_message" />
</resources>
